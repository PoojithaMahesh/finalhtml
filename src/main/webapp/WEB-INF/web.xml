<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
	<display-name>Archetype Created Web Application</display-name>
	<servlet>
		<servlet-name>save</servlet-name>
		<servlet-class>studentwithhtml.controller.SignUpServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>save</servlet-name>
		<url-pattern>/signup</url-pattern>
	</servlet-mapping>

	<servlet>
		<servlet-name>login</servlet-name>
		<servlet-class>studentwithhtml.controller.LoginServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>login</servlet-name>
		<url-pattern>/login</url-pattern>
	</servlet-mapping>

	<servlet>
		<servlet-name>find</servlet-name>
		<servlet-class>studentwithhtml.controller.SelectServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>find</servlet-name>
		<url-pattern>/select</url-pattern>
	</servlet-mapping>

	<servlet>
		<servlet-name>remove</servlet-name>
		<servlet-class>studentwithhtml.controller.DeleteServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>remove</servlet-name>
		<url-pattern>/delete</url-pattern>
	</servlet-mapping>

	<servlet>
		<servlet-name>updateuser</servlet-name>
		<servlet-class>studentwithhtml.controller.UpdateServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>updateuser</servlet-name>
		<url-pattern>/update</url-pattern>
	</servlet-mapping>


	<servlet>
		<servlet-name>edituser</servlet-name>
		<servlet-class>studentwithhtml.controller.EditServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>edituser</servlet-name>
		<url-pattern>/edit</url-pattern>
	</servlet-mapping>
</web-app>
